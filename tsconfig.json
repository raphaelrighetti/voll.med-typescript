{
  "compilerOptions": {
    "target": "es2016",
    "module": "ES6",
    "outDir": "./app/dist/js",
    "noEmitOnError": true,                            /* Disable emitting files if any type checking errors are reported. */
    "strictNullChecks": true,
    "noImplicitAny": true,
    "experimentalDecorators": true
},
  "include": ["./app/src/**/*"]
}
